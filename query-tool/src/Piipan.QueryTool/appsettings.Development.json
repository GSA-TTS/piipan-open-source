{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AuthorizationPolicy": {
    "RequiredClaims": [
        {
            "Type": "email",
            "Values": [ "*" ]
        },
        {
            "Type": "role",
            "Values": [ "Location-*" ]
        },
        {
            "Type": "role",
            "Values": [ "Role-*" ]
        }
    ]
  }
}
